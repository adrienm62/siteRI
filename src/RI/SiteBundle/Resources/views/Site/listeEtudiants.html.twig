{# src/SiteRI/SiteBundle/Resources/views/Site/partenaire.html.twig #}
{% extends "RISiteBundle::layout.html.twig" %}
{% block title %}
    Stagiaires - {{ parent() }}
{% endblock %}
    
{%block body%}
    <ul>
    {%for etudiant in etudiants %}
        {%if etudiant.ine is not null %}
            {{ etudiant.nom }} {{ etudiant.prenom }} - {{etudiant.formation }}
        {%endif%}
    {%endfor%}
    </ul>
{%endblock%}
    